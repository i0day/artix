# Launch brave
"brave"
  alt + control + b

# Launch Virt-manager
"virt-manager"
  alt + control + v

# Launch Rhythmbox
"rhythmbox"
  alt + control + r

# Launch Blueman-manager
"blueman-manager"
  alt + control + n

# Launch Pavucontrol
"pavucontrol"
  alt + control + p

# Launch Transmission-qt
"transmission-qt"
  alt + control + t

# Launch cliptask
"~/cliptask"
  alt + control + a

# Launch term
"~/term"
  alt + control + k

# Launch bt download script
"~/bt"
  alt + control + d

